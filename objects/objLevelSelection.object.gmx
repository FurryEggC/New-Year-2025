          <argument>
            <kind>1</kind>
            <string>hue++;

if (scrButtonCheckPressed(global.leftButton)) level--;

if (scrButtonCheckReleased(global.leftButton)) time_l = 0;

if (scrButtonCheckPressed(global.rightButton)) level++;

if (scrButtonCheckReleased(global.rightButton)) time_r = 0;

if (scrButtonCheck(global.leftButton))
{
    time_l++;
    if (time_l &gt;= 25 &amp;&amp; time_l &lt; 95 &amp;&amp; !(time_l % 25)) {level--;}
    else if (time_l &gt;= 95 &amp;&amp; time_l &lt; 150 &amp;&amp; !(time_l % 15)) {level--;}
    else if (time_l &gt;= 150 &amp;&amp; time_l &lt; 250 &amp;&amp; !(time_l % 10)) {level--;}
    else if (time_l &gt;= 250 &amp;&amp; !(time_l % 5)) {level--;}
}

if (scrButtonCheck(global.rightButton))
{
    time_r++;
    if (time_r &gt;= 25 &amp;&amp; time_r &lt; 95 &amp;&amp; !(time_r % 25)) {level++;}
    else if (time_r &gt;= 95 &amp;&amp; time_r &lt; 150 &amp;&amp; !(time_r % 15)) {level++;}
    else if (time_r &gt;= 150 &amp;&amp; time_r &lt; 250 &amp;&amp; !(time_r % 10)) {level++;}
    else if (time_r &gt;= 250 &amp;&amp; !(time_r % 5)) {level++;}
}

if (scrButtonCheckPressed(global.shootButton)) room_goto(rEndRoom);

if (scrButtonCheckPressed(global.jumpButton)) room_goto(rBegining + level);

if (level == 51) level = 0;
else if (level == -1) level = 50;
</string>
